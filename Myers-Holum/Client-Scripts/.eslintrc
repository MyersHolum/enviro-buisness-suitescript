{
    "env": {
      "browser": true,
      "es6": false,
      "node": false
    },
    "globals": {
      "define": true,
      "log": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "script"
    },
    "rules": {
      "import/no-amd": 0,
      "prefer-arrow-callback": 0,
      "func-names": 0,
      "no-var": 0,
      "comma-dangle": 0,
      "object-shorthand": 0,
      "linebreak-style": 0,
      "prefer-template": 0,
      "prefer-destructuring": 0,
      "vars-on-top": 0,
      "no-use-before-define": 0,
      "consistent-return": ["error", { "treatUndefinedAsUnspecified": true }],
      "eqeqeq": 0,
      "strict": ["error", "never"],
      "space-before-function-paren": 0,
      "no-loop-func": 0,
      "no-underscore-dangle": 0,
      "no-lonely-if": 0
    }
  }